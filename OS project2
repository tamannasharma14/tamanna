import java.util.*;
class student
{
int pid,burst_time,arrival_time,completion_time,waiting_time,tat,bt_total;
student(int a,int b,int c)
   {
    pid=a;
    burst_time=b;
    arrival_time=c;
     bt_total=b;
   }
}
class TestStudent
{
int n;
ArrayList<student> obj=new ArrayList<student>();

public static void main(String ar[])
{
System.out.println("Enter the no.of processes");
