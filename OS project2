Scanner sd=new Scanner(System.in);
for(i=0;i<n;i++)
{
int pid=sd.nextInt();
int burst_time=sd.nextInt();
int arrival_time=sd.nextInt();
obj.add(new student(pid,burst_time,arrival_time);
}
}

